<body>

  <h1>Videos</h1>

  <div>
    <ul style="list-style-type:none">
      <% videos.forEach(video => { %>
        <li><%= video.title %></li>
        <li><iframe width="560" height="315" src="https://www.youtube.com/embed/<%= video.video %>" frameborder="0" allowfullscreen></iframe></li>
      <% }); %>
    </ul>
  </div>

</body>
