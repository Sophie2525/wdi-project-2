<!DOCTYPE html>
<html>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta charset="utf-8">
  <title>WDI Project 2</title>
  <link rel="stylesheet" href="/css/main.css">
  <script
  src="https://code.jquery.com/jquery-3.2.1.min.js"
  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
  crossorigin="anonymous"></script>
  <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBHLVnNmjiWCU6dGd10eea4_rLsjLfghME&callback=initMap"
 type="text/javascript"></script>
  <script src="/js/client.js" charset="utf-8"></script>
</head>
<body>

  <header>
    <ul>
      <li><a href="/">Fan Engagement</a></li>
      <li><a href="/venues">Media Map</a></li>
      <li><a href="/venues/new">Add Media</a></li>
      <% if (locals.isLoggedIn) { %>
      <li><a href="/logout">Logout</a></li>
      <% } else { %>
      <li><a href="/login">Log In</a></li>
      <li><a href="/register">Register</a></li>
      <% } %>
    </ul>
  </header>

  <main>
    <% for (const type in messages) { %>
      <p class="alert alert-<%= type %>"><%= messages[type] %></p>
    <% } %>
    <%- body %>
  </main>

  <footer></footer>
</body>
</html>
