<div class="formContainer" id="new">
  <h1>Add Media</h1>

  <form method="post" action="/venues">

    <div class="form-group">
      <label for="title">Title</label>
      <input type="text" id="title" name="title" class="input">
    </div>

    <div class="form-group">
      <label for="description">Description</label>
      <textarea id="description" name="description" class="input"></textarea>
    </div>

    <div class="form-group">
      <label for="image">Image Link</label>
      <input type="text" id="image" name="image" class="input">
    </div>

    <div class="form-group">
      <label for="location">Location</label>
      <input type="text" id="location" name="location" class="input">
    </div>

    <div class="form-group">
      <label for="latitude">Latitude</label>
      <input type="text" id="latitude" name="lat" class="input">
    </div>

    <div class="form-group">
      <label for="longitude">Longitude</label>
      <input type="text" id="longitude" name="lng" class="input">
    </div>

    <button type="submit" class="button">Submit</button>
  </form>
</div>
